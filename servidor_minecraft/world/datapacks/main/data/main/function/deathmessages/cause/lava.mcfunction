execute if score #random value matches 1..2 run tellraw @a [{selector:"@s"},{text:" tried to swim in lava"}]
execute if score #random value matches 3..4 run tellraw @a [{selector:"@s"},{text:" became too hot"}]
execute if score #random value matches 5 run tellraw @a [{selector:"@s"},{text:" clearly isn't fireproof"}]
execute if score #random value matches 6 run tellraw @a [{selector:"@s"},{text:" died in lava"}]
execute if score #random value matches 7 run tellraw @a [{selector:"@s"},{text:" forgot the floor was lava"}]
execute if score #random value matches 8 run tellraw @a [{selector:"@s"},{text:" forgot to bring water"}]
execute if score #random value matches 9 run tellraw @a [{selector:"@s"},{text:" isn't fire resistant"}]
execute if score #random value matches 10 run tellraw @a [{selector:"@s"},{text:" neglected the presence of lava"}]
execute if score #random value matches 11 run tellraw @a [{selector:"@s"},{text:" burned to a crisp"}]
execute if score #random value matches 12 run tellraw @a [{selector:"@s"},{text:" likes lava. A bit too much even"}]
execute if score #random value matches 13 run tellraw @a [{selector:"@s"},{text:" burned up"}]
execute if score #random value matches 14..15 run tellraw @a [{selector:"@s"},{text:" went up in flames"}]