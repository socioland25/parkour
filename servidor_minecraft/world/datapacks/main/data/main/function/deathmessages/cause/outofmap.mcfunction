execute if score #random value matches 5..6 run tellraw @a [{selector:"@s"},{text:" was caught escaping the map"}]
execute if score #random value matches 7..8 run tellraw @a [{selector:"@s"},{text:" went somewhere they shouldn't go"}]
execute if score #random value matches 9..10 run tellraw @a [{selector:"@s"},{text:" went the wrong way"}]
execute if score #random value matches 11..12 run tellraw @a [{selector:"@s"},{text:" wasn't allowed to go there"}]
